<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/">
    <ShortName>Google Translate</ShortName>
    <Description>Google Translate</Description>
    <InputEncoding>UTF-8</InputEncoding>
    <Image width="16" height="16">data:image/png;base64,</Image>
    <Url type="text/html" method="POST" template="https://translate.google.com/" resultDomain="translate.google.com">
        <Param name="sl" value="auto"/>
        <Param name="tl" value="en"/>
        <Param name="js" value="y"/>
        <Param name="prev" value="_t"/>
        <Param name="hl" value="en"/>
        <Param name="ie" value="UTF-8"/>
        <Param name="text" value="{searchTerms}"/>
        <Param name="edit-text" value=""/>
    </Url>
</OpenSearchDescription>
