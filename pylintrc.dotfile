[BASIC]
const-rgx=(([A-Z_][A-Z0-9_]*)|(__.*__)|logger)$
variable-rgx=(([a-z_][a-z0-9_]{2,30})|ip)$

[DESIGN]
max-args=8

[MESSAGES CONTROL]

# Use `pylint --help-msg=` to check the meaning of these
disable=bad-builtin,broad-except,fixme,line-too-long,missing-docstring,no-self-use,similarities,star-args,too-few-public-methods,too-many-public-methods,unused-wildcard-import

[VARIABLES]

dummy-variables-rgx=_.*|dummy

